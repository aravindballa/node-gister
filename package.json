{
  "name": "gister-cli",
  "version": "1.0.0",
  "description": "A simple node package which creates GitHub Gist based on files.",
  "preferGlobal": true,
  "bin": {
    "gister": "build/gister.js"
  },
  "main": "build/gister.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel src -d build",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "keywords": [
    "node",
    "gist",
    "github"
  ],
  "author": "Aravind Balla (http://aravindballa.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aravindballa/node-gister/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindballa/node-gister"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "commander": "^2.12.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "watch": "^1.0.2"
  }
}
